    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.7",
    "ts-jest": "^29.1.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.15.0",
    "vite": "^5.3.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "npm run lint:fix"
